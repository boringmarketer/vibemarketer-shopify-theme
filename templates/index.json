      "custom_css": [
        ".image-banner { min-height: 90vh; position: relative; overflow: hidden; background: var(--gradient-light); }",
        ".image-banner::before { content: ''; position: absolute; top: 0; left: 0; right: 0; bottom: 0; background: radial-gradient(circle at 50% 50%, rgba(37, 99, 235, 0.1) 0%, transparent 50%); z-index: 1; }",
        ".banner__content { position: relative; z-index: 2; text-align: center; max-width: 900px; margin: 0 auto; padding: var(--space-8); }",
        ".banner__heading { font-family: var(--font-display); font-size: clamp(3rem, 8vw, 6rem); font-weight: 700; line-height: 1.1; letter-spacing: -0.02em; color: var(--color-gray-900); margin-bottom: var(--space-6); }",
        ".banner__text { font-size: clamp(1.125rem, 1.5vw, 1.25rem); line-height: 1.7; color: var(--color-gray-600); margin-bottom: var(--space-8); max-width: 600px; margin-left: auto; margin-right: auto; }",
        ".banner__buttons { display: flex; gap: var(--space-4); justify-content: center; flex-wrap: wrap; }",
        ".banner__buttons .button { font-family: var(--font-primary); font-weight: 500; border-radius: var(--radius-md); transition: var(--transition-fast); min-height: 52px; padding: var(--space-4) var(--space-8); }",
        ".banner__buttons .button--primary { background: var(--gradient-primary); border: none; }",
        ".banner__buttons .button--primary:hover { transform: translateY(-1px); box-shadow: var(--shadow-lg); }",
        ".banner__buttons .button--secondary { background: var(--color-white); color: var(--color-gray-800); border: 2px solid var(--color-gray-300); }",
        ".banner__buttons .button--secondary:hover { background: var(--color-gray-50); border-color: var(--color-gray-400); }",
        "@media (max-width: 768px) { .banner__buttons { flex-direction: column; align-items: center; } .banner__buttons .button { width: 100%; max-width: 280px; } }"
      ],
